!/bin/bash
case "$1" in
   "c") bash ./c > output.txt
   ;;
   "java") bash ./java > output.txt
   ;;
   "python") bash ./python > output.txt
   ;;
   *) bash c > output.txt
      bash java >> output.txt
      bash python >> output.txt
   ;;
esac
echo "!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>"your choice" </title>
</head>
<body>
    <p> "$(cat output.txt)" </p>
</body>
</html>
">index.html
echo "print after command____________"
cat index.html
rm output.txt
ls
echo "FROM httpd:2.4" > Dockerfile
echo "COPY . /usr/local/apache2/htdocs/" >> Dockerfile
ls
